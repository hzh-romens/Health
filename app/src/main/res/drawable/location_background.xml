<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/location_background_pressed" android:state_pressed="true" />
    <item android:drawable="@drawable/location_background_enable" android:state_enabled="false" />
    <item>
        <shape android:dither="true" android:shape="rectangle">

            <stroke android:width="1dp" android:color="#ffeeeeee" />

            <solid android:color="#ffeeeeee" />

            <corners android:radius="2dp" />
        </shape>
    </item>
</selector>